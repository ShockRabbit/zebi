    "rbenv": {
        "global": "2.6.5",
        "versions": [
            {
                "version": "2.6.5",
                "packages": [
                    "neovim",
                    "teamocil"
                ]
            }
        ]
    },
    "nodenv": {
        "global": "14.4.0",
        "versions": [
            {
                "version": "14.4.0",
                "packages": [
                    "neovim"
                ]
            }
        ]
    },
    "git_organization": [
        {
            "name": "cs",
            "url": "gitlab.custom.com",
            "user_name": "tester",
            "user_email": "tester@custom.com",
            "private_token": "test_private_token",
            "include_paths": [
                "~/Temp",
                "~/Custom"
            ]
        },
        {
            "name": "gh_sr",
            "url": "github.com",
            "user_name": "ShockRabbit",
            "user_email": "sr@email.com",
            "private_token": "test_private_token",
            "include_paths": [
                "~/ShockRabbit"
            ]
        }
    ],
    "git_clone": [
        {
            "name": "shockrabbit-installer",
            "type": "repository",
            "ssh_url": "git@github.com:ShockRabbit/installer.git",
            "branch": "master",
            "recursive": false,
            "dest_path": "~/ShockRabbit/installer"
        },
        {
            "name": "my-gitlab-group",
            "type": "gitlab-group",
            "group_id": "117",
            "private_token": "my_token",
            "root_url": "gitlab.custom.com",
            "branch": "master",
            "recursive": true,
            "dest_path": "~/MyGitlabGroup"
        },
        {
            "name": "my-github",
            "type": "github-user",
            "user_name": "shockrabbit",
            "private_token": "my_token",
            "branch": "develop",
            "recursive": true,
            "dest_path": "~/ShockRabbit"
        }
    ],
