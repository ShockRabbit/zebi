{
    "order": [
        "pyenv",
        "rbenv",
        "nodenv",
        "git_organization",
        "git_clone",
        "mas",
        "brew",
        "unity3d",
        "android_cmdline_tools",
        "sdkman",
        "downloader",
        "unzip",
        "~/your_dir/example.sh"
