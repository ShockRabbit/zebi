    "pyenv": {
        "global": "default",
        "versions": [
            {
                "version": "2.7.17",
                "envs": [
                    {
                        "name": "analytics",
                        "packages": [
                            "gspread",
                            "oauth2client",
                            "pyyaml"
                        ]
                    },
                    {
                        "name": "neovim2",
                        "packages": [
                            "neovim"
                        ]
                    }
                ]
            },
            {
                "version": "3.8.1",
                "envs": [
                    {
                        "name": "default",
                        "packages": [
                            "mdv",
                            "tabulate",
                            "flake8",
                            "numpy",
                            "pandas",
                            "gspread",
                            "matplotlib",
                            "python-gitlab",
